{
	"name": "contributte/firewall",
	"description": "Multi module Authentication and Authorization for Nette Framework",
	"type": "library",
	"license": "MIT",
	"homepage": "https://github.com/contributte/firewall",
	"require": {
		"php": ">=8.1",
		"nette/di": "^3.1.8",
		"nette/http": "^3.2.3",
		"nette/security": "^3.1.2",
		"latte/latte": "^3.0.12",
		"tracy/tracy": "^2.10.5"
	},
	"require-dev": {
		"ninjify/nunjuck": "^0.4",
		"ninjify/qa": "^0.12",
		"phpstan/phpstan": "^1.0",
		"phpstan/phpstan-nette": "^1.0",
		"phpstan/phpstan-strict-rules": "^1.0",
		"phpstan/phpstan-deprecation-rules": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Contributte\\Firewall\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Fixtures\\": "tests/fixtures",
			"Tests\\Toolkit\\": "tests/toolkit"
		}
	},
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
